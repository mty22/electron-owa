{
    "name": "electron-owa",
    "version": "1.0.1",
    "description": "Electron wrapper for Outlook Web App",
    "productName": "Outlook Web App",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-win": "electron-packager . --platform=win32 --icon=icon.ico --ignore=node_modules/bab.* --overwrite",
        "package-lin": "electron-packager . --platform=linux --icon=icon.ico --ignore=node_modules/bab.* --overwrite"
    },
    "repository": "https://github.com/Doccrazy/electron-owa",
    "keywords": [
        "Electron",
        "Outlook",
        "OWA"
    ],
    "author": "Doccrazy",
    "license": "MIT",
    "devDependencies": {
        "atob": "^2.0.3",
        "btoa": "^1.1.2",
        "electron": "^19.0.0",
        "electron-packager": "^15.5.0",
        "electron-rebuild": "^3.2.7",
        "node-webcrypto-ossl": "^2.1.3"
    },
    "dependencies": {
        "asn1js": "^3.0.5",
        "edge": "^7.10.1",
        "electron-window-state": "^5.0.3",
        "emailjs-addressparser": "^2.0.3",
        "emailjs-mime-codec": "^1.0.3",
        "emailjs-mime-parser": "^2.0.6",
        "pkijs": "^3.0.5",
        "pvutils": "^1.1.3"
    }
}